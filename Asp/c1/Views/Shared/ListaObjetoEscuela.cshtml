 
 <table class="table table-dark table-hover">
     <thead>
         <th scope="col">Nombre</th>
         <th scope="col">Id</th>
     </thead>
     <tbody>
          @foreach (var reg in Model)
          {
            <tr>
                <td>@reg.Nombre</td>
                <td>@reg.UniqueId</td>
            </tr>    
          }
     </tbody>
 </table>